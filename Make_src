      mo_IO.f90\
      mo_ckinput.f90\
      Mumps_Mod.f90\
      fp_parameters.f90\
      fparser.f90\
      Rates_Mod.f90\
      issa_Mod.f90\
      Rosenbrock_Mod.f90\
      Integration_Mod.f90\
      AtCSol.f90
      


      
$(LIB2)/lib$(CHEM).a: $(OBJ)
	@if [ -f *.o ] ;\
          then  rm *.o ;\
   fi
   
	$(F90) $(OPT2)  -I. $(KINCL) -c $?
	@ar $(X64) rv $(LIB2)/lib$(CHEM).a ${?:.f90=.o}
	@ar $(X64) ts $(LIB2)/lib$(CHEM).a
	-@for i in `ls *.mod 2>/dev/null`; do \
           mv $$i $(LIB2); \
        done
	@rm *.o

